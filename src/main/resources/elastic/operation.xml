<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE elasticMapper SYSTEM "elasticmapping.dtd">
<elasticMapper name="operation" desc="病案首页手术部分">
    <!--doc id 通过病案首页ID + 手术时间 +  手术编码-->
    <propertys>
        <property sourceName="documentid" targetName="Oper_id" desc="手术ID" idField="true"></property>
        <property sourceName="patientid" targetName="Oper_AdmNo" desc="就诊ID" rowKey="true" parentField="true"></property>
        <property sourceName="visitnumber" targetName="Oper_RegNo" desc="登记号" routingField="true"></property>
        <property sourceName="OperName" targetName="Oper_Name" desc="手术名称"></property>
        <property sourceName="OperDate" targetName="Oper_StartTime" desc="手术开始时间"></property>
        <property sourceName="OperCode" targetName="Oper_GenericCode" desc="手术归一编码"></property>
        <property sourceName="OperName" targetName="Oper_GenericName" desc="手术归一名称"></property>
        <property sourceName="OperLevel" targetName="Oper_Level" desc="手术级别"></property>
        <property sourceName="AnaesWay" targetName="Oper_AnaesWay" desc="麻醉方式"></property>
        <property sourceName="NcisionHealingLevel" targetName="Oper_NcisionHealingLevel" desc="切口愈合等级"></property>
        <property sourceName="Operator" targetName="Oper_Operator" desc="术者"></property>
        <property sourceName="OperaAssistantFir" targetName="Oper_OperaAssistantFir" desc="助1"></property>
        <property sourceName="OperaAssistantSec" targetName="Oper_OperaAssistantSec" desc="助2"></property>
        <property sourceName="AnaesDoc" targetName="Oper_AnaesDoc" desc="麻醉医师"></property>
        <property sourceName="IsCurOpera" targetName="Oper_IsCurOpera" desc="是否本次手术"></property>
    </propertys>
</elasticMapper>