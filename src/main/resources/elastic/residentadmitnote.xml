    <!--现病史-->
    <property sourceName="xianbingshi" targetName="RAN_PresentIllness" convert="false" valueType="java.lang.String">
    </property>
    <!--既往史-->
    <property sourceName="jiwangshi" targetName="RAN_PastHistory" convert="false" valueType="java.lang.String">
    </property>
    <!--个人史-->
    <property sourceName="gerenshi" targetName="RAN_Sociology" convert="false" valueType="java.lang.String">
    </property>
    <!--婚育史-->
    <property sourceName="yjhyshi" targetName="RAN_Marriage" convert="false" valueType="java.lang.String">
    </property>
    <!--家族史-->
    <property sourceName="jiazushi" targetName="RAN_Family" convert="false" valueType="java.lang.String">
