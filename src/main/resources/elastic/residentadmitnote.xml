<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE elasticMapper SYSTEM "elasticmapping.dtd">
<elasticMapper name="residentadmitnote" desc="导入入院记录数据">
    <propertys>
        <property sourceName="documentid"  targetName="RAN_Id" valueType="String" idField="true"></property>
        <!--就诊号ID-->
        <property sourceName="visitnumber" targetName="RAN_AdmNo" valueType="String" rowKey="true" parentField="true"></property>
        <!--登记号-->
        <property sourceName="patientid" targetName="RAN_RegNo"  valueType="String" routingField="true"></property>
        <!--主诉-->
        <property sourceName="zs" targetName="RAN_Complain" valueType="String"></property>
        <!--现病史-->
        <property sourceName="xbs" targetName="RAN_PresentIllness" valueType="String"></property>
        <!--既往史-->
        <property sourceName="jws" targetName="RAN_PastHistory" valueType="String"></property>
        <!--个人史-->
        <property sourceName="grs" targetName="RAN_Sociology" valueType="String"></property>
        <!--婚育史-->
        <property sourceName="yjhys" targetName="RAN_Marriage" valueType="String"></property>
        <!--家族史-->
        <property sourceName="jzs" targetName="RAN_Family" valueType="String"></property>
        <!--体格检查-->
        <property sourceName="tgjc" targetName="RAN_PhysicalExamination" valueType="String"></property>
        <!--专科检查-->
        <property sourceName="zkqkjc" targetName="RAN_SpecialityExamination" valueType="String"></property>
        <!--辅助检查-->
        <property sourceName="fzjc" targetName="RAN_SupplementaryExamination" valueType="String"></property>
    </propertys>

    <outputs>
        <output>
            <elasticsearch index="csmsearch" type="residentadmitnote"></elasticsearch>
        </output>
    </outputs>
</elasticMapper>