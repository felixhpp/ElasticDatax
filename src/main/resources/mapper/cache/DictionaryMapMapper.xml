    </select>
    <!-- 就诊状态-->
    <select id="getAdmStatusByCode" resultMap="AdmStatusMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_EncounterStatus u WHERE 1 = 1 AND CTES_Code = #{code}
    </select>

    <!-- 查询所有诊断字典 -->
    <select id="getAllDiagnose" resultMap="DiagnoseMap">
        SELECT * FROM  mdm_USER.CT_Diagnose
    </select>
    <!-- 根据编码查询诊断字典-->
    <select id="getDiagnoseByCode" resultMap="DiagnoseMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_Diagnose u WHERE 1 = 1 AND CTD_Code = #{code}
    </select>

    <!-- 查询所有诊断类型字典 -->
    <select id="getAllDiagnoseType" resultMap="DiagnoseTypeMap">
        SELECT * FROM  mdm_USER.CT_DiagnoseType
    </select>
    <!-- 根据编码查询诊断类型字典-->
    <select id="getDiagnoseTypeByCode" resultMap="DiagnoseTypeMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_DiagnoseType u WHERE 1 = 1 AND CTDT_Code = #{code}
    </select>

    <!--就诊类型-->
    <resultMap id="AdmTypeMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTET_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTET_Desc" property="dicName" jdbcType="VARCHAR" />
    </resultMap>
    <!-- 查询所有就诊类型字典 -->
    <select id="getAllAdmType" resultMap="AdmTypeMap">
        SELECT * FROM  mdm_USER.CT_EncounterType
    </select>
    <!-- 根据编码查询就诊类型字典-->
    <select id="getAdmTypeByCode" resultMap="AdmTypeMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_EncounterType u WHERE 1 = 1 AND CTET_Code = #{code}
    </select>

    <!--检验项目字典表-->
    <resultMap id="LisItemMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTLI_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTLI_Desc" property="dicName" jdbcType="VARCHAR" />
    </resultMap>
    <!-- 查询所有检验项目字典 -->
    <select id="getAllLisItem" resultMap="LisItemMap">
        SELECT * FROM  mdm_USER.CT_LisItem
    </select>
    <!-- 根据编码查询检验项目字典-->
    <select id="getLisItemByCode" resultMap="LisItemMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_LisItem u WHERE 1 = 1 AND CTLI_Code = #{code}
    </select>

</mapper>