    </select>
    <!-- 根据编码查询科室-->
    <select id="getDeptByCode" resultMap="DeptMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_Dept u WHERE 1 = 1 AND CTD_Code = #{code}
    </select>

    <!-- 查询所有医院 -->
    <select id="getAllHospital" resultMap="HospitalMap">
        SELECT * FROM  mdm_USER.CT_Hospital
    </select>
    <!-- 根据编码查询医院-->
    <select id="getHospitalByCode" resultMap="HospitalMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_Hospital u WHERE 1 = 1 AND CTH_Code = #{code}
    </select>


    <!--就诊状态-->
    <resultMap id="AdmStatusMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTES_Code" property="dicCode" jdbcType="VARCHAR" />
