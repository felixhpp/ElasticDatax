<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.demo.mapper.cache.DictionaryMapMapper" >
    <!-- 性别映射 -->
    <resultMap id="SexMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTS_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTS_Desc" property="dicName" jdbcType="VARCHAR" />
    </resultMap>
    <!--婚姻状态-->
    <resultMap id="MaritalMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTM_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTM_Desc" property="dicName" jdbcType="VARCHAR" />
    </resultMap>
    <!--民族-->
    <resultMap id="NationMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTN_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTN_Desc" property="dicName" jdbcType="VARCHAR" />
    </resultMap>
    <!--科室-->
    <resultMap id="DeptMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTD_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTD_Desc" property="dicName" jdbcType="VARCHAR" />
    </resultMap>
    <!--医院-->
    <resultMap id="HospitalMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTH_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTH_Desc" property="dicName" jdbcType="VARCHAR" />
    </resultMap>
    <!--诊断-->
    <resultMap id="DiagnoseMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTD_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTD_Desc" property="dicName" jdbcType="VARCHAR" />
        <result column="CTD_ICDCode" property="dicICDCode" jdbcType="VARCHAR" />
    </resultMap>
    <!--诊断类型-->
    <resultMap id="DiagnoseTypeMap" type="com.example.demo.entity.DictionaryMap">
        <!--column数据库中表的名  property是对应类的属性  -->
        <result column="CTDT_Code" property="dicCode" jdbcType="VARCHAR" />
        <result column="CTDT_Desc" property="dicName" jdbcType="VARCHAR" />
    </resultMap>


    <!-- 查询所有性别 -->
    <select id="getAllSex" resultMap="SexMap">
        SELECT * FROM  mdm_USER.CT_Sex
    </select>
    <!-- 根据编码查询性别 -->
    <select id="getSexByCode" resultMap="SexMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_Sex u WHERE 1 = 1 AND CTS_Code = #{code}
    </select>

    <!-- 查询所有婚姻状态 -->
    <select id="getAllMarital" resultMap="MaritalMap">
        SELECT * FROM  mdm_USER.CT_Marital
    </select>
    <!-- 根据编码查询婚姻状态-->
    <select id="getMaritalByCode" resultMap="MaritalMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_Marital u WHERE 1 = 1 AND CTM_Code = #{code}
    </select>

    <!-- 查询所有民族 -->
    <select id="getAllNation" resultMap="NationMap">
        SELECT * FROM  mdm_USER.CT_Nation
    </select>
    <!-- 根据编码查询民族-->
    <select id="getNationByCode" resultMap="NationMap" parameterType="java.util.Map">
        SELECT * FROM mdm_USER.CT_Nation u WHERE 1 = 1 AND CTN_Code = #{code}
    </select>

    <!-- 查询所有科室 -->
