@echo off

set ERROR_CODE=0

:init
@REM Decide how to startup depending on the version of windows

@REM -- Win98ME
if NOT "%OS%"=="Windows_NT" goto Win9xArg

@REM set local scope for the variables with windows NT shell
if "%OS%"=="Windows_NT" @setlocal

@REM -- 4NT shell
if "%eval[2+2]" == "4" goto 4NTArgs

@REM -- Regular WinNT shell
set CMD_LINE_ARGS=%*
goto WinNTGetScriptDir

@REM The 4NT Shell from jp software
:4NTArgs
set CMD_LINE_ARGS=%$
goto WinNTGetScriptDir

:Win9xArg
@REM Slurp the command line arguments.  This loop allows for an unlimited number
@REM of arguments (up to the command line limit, anyway).
set CMD_LINE_ARGS=
:Win9xApp
if %1a==a goto Win9xGetScriptDir
set CMD_LINE_ARGS=%CMD_LINE_ARGS% %1
shift
goto Win9xApp

:Win9xGetScriptDir
set SAVEDIR=%CD%
%0\
cd %0\..\.. 
set BASEDIR=%CD%
cd %SAVEDIR%
set SAVE_DIR=
goto repoSetup

:WinNTGetScriptDir
for %%i in ("%~dp0..") do set "BASEDIR=%%~fi"

:repoSetup
set REPO=


if "%JAVACMD%"=="" set JAVACMD=java

if "%REPO%"=="" set REPO=%BASEDIR%\lib

set CLASSPATH="%BASEDIR%"\config;"%REPO%"\spring-boot-starter-web-2.1.5.RELEASE.jar;"%REPO%"\spring-boot-starter-2.1.5.RELEASE.jar;"%REPO%"\javax.annotation-api-1.3.2.jar;"%REPO%"\spring-boot-starter-json-2.1.5.RELEASE.jar;"%REPO%"\jackson-databind-2.9.8.jar;"%REPO%"\jackson-datatype-jdk8-2.9.8.jar;"%REPO%"\jackson-datatype-jsr310-2.9.8.jar;"%REPO%"\jackson-module-parameter-names-2.9.8.jar;"%REPO%"\spring-boot-starter-tomcat-2.1.5.RELEASE.jar;"%REPO%"\tomcat-embed-core-9.0.19.jar;"%REPO%"\tomcat-embed-el-9.0.19.jar;"%REPO%"\tomcat-embed-websocket-9.0.19.jar;"%REPO%"\hibernate-validator-6.0.16.Final.jar;"%REPO%"\validation-api-2.0.1.Final.jar;"%REPO%"\jboss-logging-3.3.2.Final.jar;"%REPO%"\spring-web-5.1.7.RELEASE.jar;"%REPO%"\spring-webmvc-5.1.7.RELEASE.jar;"%REPO%"\spring-aop-5.1.7.RELEASE.jar;"%REPO%"\spring-expression-5.1.7.RELEASE.jar;"%REPO%"\spring-boot-starter-log4j2-2.1.5.RELEASE.jar;"%REPO%"\log4j-slf4j-impl-2.11.2.jar;"%REPO%"\log4j-api-2.11.2.jar;"%REPO%"\log4j-core-2.11.2.jar;"%REPO%"\log4j-jul-2.11.2.jar;"%REPO%"\jul-to-slf4j-1.7.26.jar;"%REPO%"\jackson-dataformat-yaml-2.9.8.jar;"%REPO%"\snakeyaml-1.23.jar;"%REPO%"\jackson-core-2.9.8.jar;"%REPO%"\mybatis-spring-boot-starter-2.0.1.jar;"%REPO%"\mybatis-spring-boot-autoconfigure-2.0.1.jar;"%REPO%"\mybatis-3.5.1.jar;"%REPO%"\mybatis-spring-2.0.1.jar;"%REPO%"\spring-boot-starter-thymeleaf-2.1.5.RELEASE.jar;"%REPO%"\thymeleaf-spring5-3.0.11.RELEASE.jar;"%REPO%"\thymeleaf-3.0.11.RELEASE.jar;"%REPO%"\attoparser-2.0.5.RELEASE.jar;"%REPO%"\unbescape-1.1.6.RELEASE.jar;"%REPO%"\thymeleaf-extras-java8time-3.0.4.RELEASE.jar;"%REPO%"\spring-context-support-5.1.7.RELEASE.jar;"%REPO%"\spring-beans-5.1.7.RELEASE.jar;"%REPO%"\spring-context-5.1.7.RELEASE.jar;"%REPO%"\spring-core-5.1.7.RELEASE.jar;"%REPO%"\spring-jcl-5.1.7.RELEASE.jar;"%REPO%"\spring-boot-devtools-2.1.5.RELEASE.jar;"%REPO%"\spring-boot-2.1.5.RELEASE.jar;"%REPO%"\spring-boot-autoconfigure-2.1.5.RELEASE.jar;"%REPO%"\ehcache-3.5.2.jar;"%REPO%"\slf4j-api-1.7.2